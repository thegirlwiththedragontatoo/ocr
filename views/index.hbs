		<link rel="stylesheet" href="stylesheets/main.css" />
		<noscript><link rel="stylesheet" href="stylesheets/noscript.css" /></noscript>
	</head>
	<body class="is-preload">

		<!-- Sidebar -->
			<section id="sidebar">
				<div class="inner">
					<nav>
						<ul>
							<li><a href="#intro">Welcome</a></li>
							<li><a href="#one" id="res">Who we are</a></li>
							<li><a href="#two">What we do</a></li>
							<li><a href="#three">Get in touch</a></li>
						</ul>
					</nav>
				</div>
			</section>

		<!-- Wrapper -->
			<div id="wrapper">
				<!-- Intro -->
					<section id="intro" class="wrapper style1 fullscreen fade-up">
						<div class="inner">
							<h1>{{title}}</h1>
                            <form action="#">
                              <div class="input-file-container">
                                <input class="input-file" id="my-file" type="file" accept="image/*" capture="user">
                                <label tabindex="0" for="my-file" class="input-file-trigger button scrolly">Select a file...</label>
                              </div>
                            </form>
                            <img id="output">
                            <div id="overlay" style="display:none;">
                                <div class="spinner"></div>
                                <br/>
                                <p>Loading...<p>
                            </div>

							<!-- <ul class="actions">
								<li><a href="#one" class="button scrolly">Learn more</a></li>
							</ul> -->
						</div>
					</section>
                    <section id="one" class="wrapper style1 fullscreen fade-up" style="
    padding: 1em 2em 1em 2em;">
                        <div class="result">
                            <p class="file-return"></p>
                        </div>
                    </section>
			</div>
            <script>
              const output = document.getElementById('output');

              function doSomethingWithFiles(fileList) {
                let file = null;

                for (let i = 0; i < fileList.length; i++) {
                  if (fileList[i].type.match(/^image\//)) {
                    file = fileList[i];
                    break;
                  }
                }

                if (file !== null) {
                  output.src = URL.createObjectURL(file);
                }
              }
            </script>

